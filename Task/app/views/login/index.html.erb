<h1>Login#index</h1>
<p>Find me in app/views/login/index.html.erb</p>

<h1>Login</h1>

<%= form_tag authenticate_path do %>
  <p>
    <%# should be username %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>

  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag :login %>
  </p>
<% end %>
